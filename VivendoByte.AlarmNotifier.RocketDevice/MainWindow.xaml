<Window x:Class="VivendoByte.AlarmNotifier.RocketDevice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VivendoByte.AlarmNotifier.RocketDevice"
        mc:Ignorable="d"
        Title="Alarm Notification Sender" Height="350" Width="525">
    <Grid Margin="4">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Button Content="Send Alarm" Width="120" x:Name="SendAlarm" Click="SendAlarm_Click" />
        </StackPanel>
    </Grid>
</Window>